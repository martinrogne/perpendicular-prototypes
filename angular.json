        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/perpendicular-services-base/tsconfig.lib.json",
            "project": "projects/perpendicular-services-base/ng-package.json"
          },
          "configurations": {
